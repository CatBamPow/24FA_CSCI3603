//Task 1
if __name__ == '__main__':
    n = int(input())
i = 0
while i in range (0, n):
        print(i ** 2)
        i = i + 1

//Task 2
if __name__ == '__main__':
    n = int(input())
    arr = map(int, input().split())
a = list(arr)
a.sort()
runnerUp=min(a)
for i in a:
    if i > runnerUp and i < max(a):
        runnerUp = i
print(runnerUp)

